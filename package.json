{
  "name": "http-proxy-with-cors",
  "version": "0.0.0-development",
  "description": "Proxy HTTP requests with CORS support",
  "keywords": [
    "proxy-server",
    "http"
  ],
  "type": "module",
  "bin": {
    "http-proxy-with-cors": "./src/index.js"
  },
  "files": [
    "src"
  ],
  "main": "src/index.js",
  "scripts": {
    "prepare": "husky install",
    "test": "vitest --run",
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\" --fix",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/http-proxy": "^9.1.0",
    "@fastify/reply-from": "^9.2.0",
    "chalk": "^5.2.0",
    "fastify": "^4.17.0",
    "pino-pretty": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@fastify/cookie": "^8.3.0",
    "@types/node": "^20.2.5",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "playwright": "^1.34.3",
    "prettier": "^2.8.8",
    "semantic-release": "^21.0.2",
    "verdaccio": "^5.25.0",
    "vitest": "^0.32.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rluvaton/http-proxy-with-cors.git"
  },
  "bugs": {
    "url": "https://github.com/rluvaton/http-proxy-with-cors/issues"
  },
  "license": "MIT"
}
